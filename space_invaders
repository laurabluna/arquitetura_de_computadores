nave:
lui $8, 0x1001
   addi $8, $8, 28928
   addi $7, $0, 0xFFD700
   sw $7, 0($8)
   addi $8, $8, 512 #27896
   sw $7, 0($8)
   addi $8, $8, 4 #27900
   sw $7, 0($8)
   addi $8, $8, -8 #27892
   sw $7, 0($8)
   addi $8, $8, 512 #28404
   sw $7, 0($8)
   addi $8, $8, -4
   sw $7, 0($8)
   addi $8, $8, 8
   sw $7, 0($8)
   addi $8, $8, 4
   sw $7, 0($8)
   addi $8, $8, 4
   sw $7, 0($8)
   
   # movimentacao da nave
d_nave:
   lui $8, 0x1001
   addi $8, $8, 28928
   addi $12, $8, 0  
   addi $7, $0, 0xFFD700
   sw $7, 0($8)
   addi $8, $8, 512 #27896
   sw $7, 0($8)
   addi $8, $8, 4 #27900
   sw $7, 0($8)
   addi $8, $8, -8 #27892
   sw $7, 0($8)
   addi $8, $8, 512 #28404
   sw $7, 0($8)
   addi $8, $8, -4
   sw $7, 0($8)
   addi $8, $8, 8
   sw $7, 0($8)
   addi $8, $8, 4
   sw $7, 0($8)
   addi $8, $8, 4
   sw $7, 0($8)
   j v_p
v_p:
	beq $13, $0, mudar_cor
	addi $7, $0, 0xFFD700
	addi $13, $0, 0
	j piscar_nave
mudar_cor:
	addi $7, $0, 0xc6072e
	addi $13, $0, 1
	j piscar_nave
piscar_nave:
	addi $8, $12, 0
   	sw $7, 0($8)
   	addi $4, $0, 300
   	addi $2, $0, 32
   	syscall
   	j verificar_teclado
verificar_teclado:
        addi $3, $8, 0 
        lw $10, 0($3) 
	lui $1, 0xffff
	lw $24, 4($1)
	beq $24, $0, v_p 
	addi $25, $0, 'd'
	beq $24, $25, moverDireita
	addi $25, $0, 'a'
	beq $24, $25, moverEsquerda
	addi $25, $0, ' '
	beq $24, $25, t
moverDireita: 
        sw $0, 4($1)
        jal apagar_nave
        li $2, 31
        addi $12, $12, 16
        j nave2
moverEsquerda:
        sw $0, 4($1)
	jal apagar_nave
	addi $12, $12, -16
	j nave2
t: 	
	sw $0, 4($1)
	addi $20, $0, 3
	addi $21, $0, 0
	addi $7, $0, 0xfb4009
	addi $8, $12, -1024
	j tiro
tiro:
	beq $20, $21, apagar_ultimo_tiro
	addi $21, $21, 1
	addi $7, $0, 0xfb4009
	jal desenhar_tiro #start here
	addi $7, $0, 0xff0000 #vermelho
	addi $3, $8, 0
	lw $5, 0($3)
	beq $7, $5, apagar_npc
	addi $3, $3, 512
	lw $5, 0($3)
	beq $7, $5, apagar_npc
	addi $3, $3, 512
	lw $5, 0($3) #finish here
	#TESTE 
	addi $7, $0, 0x0000ff #azul
	addi $3, $8, 0
	lw $6, 0($3) 
	beq $7, $6, apagar_npc
	addi $3, $3, 512
	lw $6, 0($3)
	beq $7, $6, apagar_npc
	addi $3, $3, 512
	lw $6, 0($3)
	#TESTE
	beq $7, $6, apagar_npc
	jal apagar_tiro
	addi $8, $8, -6144 #era 2048
	j tiro
apagar_npc:
	jal apagar_tiro
	addi $20, $0, 20 #15
	addi $21, $0, 0
	addi $8, $12, -13852
	addi $7, $0, 0x000000
	jal quadrado_preto
	j v_p
quadrado_preto:
	beq $20, $21, voltar
	addi $21, $21, 1
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, 512
	sw $7, 0($8)
	addi $8, $8, -7164
	j quadrado_preto
voltar:
	jr $31
desenhar_tiro:
	sw $7, 0($8)
	addi $8, $8, -512
	sw $7, 0($8)
	addi $8, $8, -512
	sw $7, 0($8)
	addi $8, $8, -512
	sw $7, 0($8)
	addi $8, $8, -512
	addi $4, $0, 200
	addi $2,$0, 32
	syscall
	jr $31
apagar_tiro:
	addi $7, $0, 0x000000
	addi $8, $8, 2048
	sw $7, 0($8)
	addi $8, $8, -512
	sw $7, 0($8)
	addi $8, $8, -512
	sw $7, 0($8)
	addi $8, $8, -512
	sw $7, 0($8)
	addi $8, $8, -512
	jr $31
apagar_ultimo_tiro:
	addi $7, $0, 0x000000
	sw $7, 0($8)
	addi $8, $8, -512
	sw $7, 0($8)
	addi $8, $8, -512
	sw $7, 0($8)
	addi $8, $8, -512
	sw $7, 0($8)
	addi $8, $8, -512
	j v_p

apagar_nave:
   addi $8, $12, 0
   addi $7, $0, 0x000000
   sw $7, 0($8)
   addi $8, $8, 512 #27896
   sw $7, 0($8)
   addi $8, $8, 4 #27900
   sw $7, 0($8)
   addi $8, $8, -8 #27892
   sw $7, 0($8)
   addi $8, $8, 512 #28404
   sw $7, 0($8)
   addi $8, $8, -4
   sw $7, 0($8)
   addi $8, $8, 8
   sw $7, 0($8)
   addi $8, $8, 4
   sw $7, 0($8)
   addi $8, $8, 4
   sw $7, 0($8)
   jr $31

nave2:
   addi $8, $12, 0
   addi $7, $0, 0xFFD700
   sw $7, 0($8)
   addi $8, $8, 512 #27896
   sw $7, 0($8)
   addi $8, $8, 4 #27900
   sw $7, 0($8)
   addi $8, $8, -8 #27892
   sw $7, 0($8)
   addi $8, $8, 512 #28404
   sw $7, 0($8)
   addi $8, $8, -4
   sw $7, 0($8)
   addi $8, $8, 8
   sw $7, 0($8)
   addi $8, $8, 4
   sw $7, 0($8)
   addi $8, $8, 4
   sw $7, 0($8)
   j v_p
   j fim
 
 
 fim:
   addi $2, $0, 10
   syscall
